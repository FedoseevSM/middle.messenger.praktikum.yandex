{
    "ts-node": {
        "files": true
    },
    "compilerOptions": {
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2018",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "allowJs": true,
        "esModuleInterop": true
    }
}
